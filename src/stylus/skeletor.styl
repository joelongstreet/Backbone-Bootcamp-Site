.skeletor
  .container
    transform scale(.6)
  img
    display block
    margin 0 auto -12px

    &:nth-child(3)
      margin-bottom -15px
    &:nth-child(4), &:nth-child(5)
      margin-bottom -17px
    &:nth-child(6)
      margin-bottom -19px
    &:nth-child(7)
      margin-bottom -20px
    &:nth-child(12)
      margin-bottom -16px
    &:nth-child(25)
      margin-bottom -42px